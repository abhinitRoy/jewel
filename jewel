<script>
$(document).on("scroll", function(){
if
($(document).scrollTop() > 150){
$("#logo").css("width", "123px","transition", "all 1s ease 0s");
$("#logo").css("padding", "0 0");
$(".navbar-inverse .navbar ul.nav li a ").css("line-height", "18px","padding","14px 15px","transition", "all 1s ease 0s");
$("#search ").css("margin", "6px 45px 15px 10px","transition", "all 1s ease 0s");
$("#cart").css("margin", "6px 0","transition", "all 1s ease 0s");
$(".navbar-inverse .navbar").css("min-height", "40px","transition", "all 1s ease 0s");
